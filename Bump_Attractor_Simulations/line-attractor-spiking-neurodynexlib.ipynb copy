    "#t = 400*ms\n",
    "#t_delta = 10*ms\n",
    "#tmin = t - t_delta\n",
    "#tmax = t + t_delta\n",
    "#get_spike_count(spike_monitor_excit, np.arange(0,1024).tolist(), tmin, tmax)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "Text(0,0.5,u'Spike rate [Hz]')"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    },
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
